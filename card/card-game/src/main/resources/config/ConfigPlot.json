[
{ "ID":80001, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"前方英雄可是<color=#2652d5>张曼成</color>？", "turn":1, "sound":"", "soundType":0 },
{ "ID":80002, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"张曼成", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10038", "leftEffect":1, "rightEffect":3, "talkingText":"没错，正是张某。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80003, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"张曼成", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10038", "leftEffect":2, "rightEffect":1, "talkingText":"<color=#2652d5>张曼成</color>，黄巾军打着黄天已死的幌子，n&所到之处，民不聊生，你可视百姓的疾苦？", "turn":1, "sound":"", "soundType":0 },
{ "ID":80004, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"张曼成", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10038", "leftEffect":2, "rightEffect":1, "talkingText":"我劝你赶紧下马投降，早早结束这场无谓的战争。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80005, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"张曼成", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10038", "leftEffect":1, "rightEffect":2, "talkingText":"呸……我不懂什么大道理，我只知道我大哥<color=#2652d5>张角</color>待我不薄，n&派我当前部正印先锋官，岂能被你三言两语吓倒了！", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80006, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"张曼成", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10038", "leftEffect":2, "rightEffect":1, "talkingText":"<color=#2652d5>张曼成</color>，我视你为英雄，才好言相劝。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80007, "groupId":1, "nPCLeftname":"_name", "nPCRightname":"张曼成", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10038", "leftEffect":1, "rightEffect":2, "talkingText":"废话少说，我<color=#2652d5>张曼成</color>也不是贪生怕死之徒。哇呀呀……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80008, "groupId":1, "nPCLeftname":"黄盖", "nPCRightname":"张曼成", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10047", "rightRes":"Plot/10038", "leftEffect":3, "rightEffect":1, "talkingText":"主公，少跟他废话，待我取他项上人头。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80009, "groupId":1, "nPCLeftname":"黄盖", "nPCRightname":"张曼成", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10047", "rightRes":"Plot/10038", "leftEffect":2, "rightEffect":1, "talkingText":"<color=#2652d5>黄盖</color>在此，拿命来……哇呀呀……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80010, "groupId":2, "nPCLeftname":"", "nPCRightname":"天星-张角", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/19003", "leftEffect":0, "rightEffect":3, "talkingText":"哼哼哼……你们这群人都是来送死的吗……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80011, "groupId":2, "nPCLeftname":"_name", "nPCRightname":"天星-张角", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19003", "leftEffect":3, "rightEffect":1, "talkingText":"<color=#2652d5>张角</color>，你自建邪教，妖言惑众，蛊惑人心，实为窥视皇帝位置。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80012, "groupId":2, "nPCLeftname":"_name", "nPCRightname":"天星-张角", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19003", "leftEffect":1, "rightEffect":2, "talkingText":"哈哈哈……皇帝轮流做，今日到我家。为何别人坐的，我却做不得……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80013, "groupId":2, "nPCLeftname":"_name", "nPCRightname":"天星-张角", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19003", "leftEffect":2, "rightEffect":1, "talkingText":"<color=#2652d5>张角</color>，你派手下四处撒播瘟疫，然后再用你的草药治疗老百姓。n&让不明真相的老百姓视你为天人，而加入太平教。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80014, "groupId":2, "nPCLeftname":"_name", "nPCRightname":"天星-张角", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19003", "leftEffect":2, "rightEffect":1, "talkingText":"你只是利用百姓来实现你的野心罢了！", "turn":1, "sound":"", "soundType":0 },
{ "ID":80015, "groupId":2, "nPCLeftname":"_name", "nPCRightname":"天星-张角", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19003", "leftEffect":1, "rightEffect":2, "talkingText":"[他怎么知道的？]n&哼哼哼……满嘴胡言！我太平道顺天行事，广济百姓。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80016, "groupId":2, "nPCLeftname":"_name", "nPCRightname":"天星-张角", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19003", "leftEffect":1, "rightEffect":2, "talkingText":"_name，你不要含血喷人，坏了我太平道的名声……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80017, "groupId":2, "nPCLeftname":"黄盖", "nPCRightname":"天星-张角", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10047", "rightRes":"Plot/19003", "leftEffect":3, "rightEffect":1, "talkingText":"哇呀呀……你这个疯道士，到让我看看你有何能耐……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80018, "groupId":2, "nPCLeftname":"黄盖", "nPCRightname":"天星-张角", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10047", "rightRes":"Plot/19003", "leftEffect":2, "rightEffect":1, "talkingText":"主公，<color=#2652d5>黄盖</color>请令前去会一会他。……哇呀呀……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80019, "groupId":2, "nPCLeftname":"徐庶", "nPCRightname":"天星-张角", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10026", "rightRes":"Plot/19003", "leftEffect":3, "rightEffect":1, "talkingText":"黄兄，且慢！不可大意……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80020, "groupId":2, "nPCLeftname":"徐庶", "nPCRightname":"天星-张角", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10026", "rightRes":"Plot/19003", "leftEffect":2, "rightEffect":1, "talkingText":"听闻《太平清领书》可以呼风唤雨，撒豆成兵，还是小心为妙……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80021, "groupId":2, "nPCLeftname":"徐庶", "nPCRightname":"天星-张角", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10026", "rightRes":"Plot/19003", "leftEffect":1, "rightEffect":2, "talkingText":"哈哈哈……你们一个都别想活……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80022, "groupId":3, "nPCLeftname":"_name", "nPCRightname":"", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"", "leftEffect":2, "rightEffect":0, "talkingText":"兄弟们，总感觉哪里不太对劲啊……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80023, "groupId":3, "nPCLeftname":"_name", "nPCRightname":"徐庶", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10026", "leftEffect":1, "rightEffect":3, "talkingText":"主公，是不是觉得这张角太不禁打了？", "turn":1, "sound":"", "soundType":0 },
{ "ID":80024, "groupId":3, "nPCLeftname":"", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":2, "leftRes":"", "rightRes":"", "leftEffect":0, "rightEffect":0, "talkingText":"[天空传来了<color=#2652d5>张角</color>的声音]  哈哈哈……哈哈哈哈……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80025, "groupId":3, "nPCLeftname":"", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":2, "leftRes":"", "rightRes":"", "leftEffect":0, "rightEffect":0, "talkingText":"你们看清楚了，这只是我的障眼法……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80026, "groupId":3, "nPCLeftname":"", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":2, "leftRes":"", "rightRes":"", "leftEffect":0, "rightEffect":0, "talkingText":"_name，我今时没空理会你们，早晚我们再见面的……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80027, "groupId":3, "nPCLeftname":"", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":2, "leftRes":"", "rightRes":"", "leftEffect":0, "rightEffect":0, "talkingText":"[<color=#2652d5>张角</color>的声音逐渐消失]  哈哈哈……  ……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80028, "groupId":3, "nPCLeftname":"_name", "nPCRightname":"徐庶", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10026", "leftEffect":1, "rightEffect":2, "talkingText":"[心生堪忧……]这<color=#2652d5>张角</color>的本事不可小觑啊……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80029, "groupId":4, "nPCLeftname":"", "nPCRightname":"命星-吕布", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/19002", "leftEffect":0, "rightEffect":3, "talkingText":"_name，能闯到这里你也算有本事了。n&不过今天就是你的死期，哼……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80030, "groupId":4, "nPCLeftname":"_name", "nPCRightname":"命星-吕布", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19002", "leftEffect":3, "rightEffect":1, "talkingText":"吕奉先，史书记载你也算是人中龙凤，怎么也逆天行事。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80031, "groupId":4, "nPCLeftname":"_name", "nPCRightname":"命星-吕布", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19002", "leftEffect":1, "rightEffect":2, "talkingText":"哼……别以为我不知道，后人把耻我为三姓家奴，今天我要改写历史。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80032, "groupId":4, "nPCLeftname":"_name", "nPCRightname":"命星-吕布", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19002", "leftEffect":2, "rightEffect":1, "talkingText":"奉先兄，史书上虽然记载你立场不坚定，但三英战吕布以一敌三也不失为一段美谈。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80033, "groupId":4, "nPCLeftname":"_name", "nPCRightname":"命星-吕布", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19002", "leftEffect":1, "rightEffect":2, "talkingText":"嗷嗷啊……嗷嗷啊……[头好痛……头好痛]", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80034, "groupId":4, "nPCLeftname":"吕绮玲", "nPCRightname":"命星-吕布", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10057", "rightRes":"Plot/19002", "leftEffect":2, "rightEffect":1, "talkingText":"主公，我父亲不太对劲，你看他的眼睛……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80035, "groupId":4, "nPCLeftname":"吕绮玲", "nPCRightname":"命星-吕布", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10057", "rightRes":"Plot/19002", "leftEffect":2, "rightEffect":1, "talkingText":"父王，你怎么了？快醒醒", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80036, "groupId":4, "nPCLeftname":"_name", "nPCRightname":"命星-吕布", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19002", "leftEffect":2, "rightEffect":1, "talkingText":"大家小心点，他好像被魔化了……", "turn":1, "sound":"", "soundType":0 },
{ "ID":80037, "groupId":4, "nPCLeftname":"_name", "nPCRightname":"命星-吕布", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19002", "leftEffect":1, "rightEffect":2, "talkingText":"嗷嗷啊……头好痛……我要杀了你们……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80038, "groupId":5, "nPCLeftname":"", "nPCRightname":"士兵", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/15001", "leftEffect":0, "rightEffect":3, "talkingText":"主公，前方探子来报，一路上伏兵四起，请主公定夺。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80039, "groupId":5, "nPCLeftname":"_name", "nPCRightname":"士兵", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/15001", "leftEffect":3, "rightEffect":1, "talkingText":"前军突进，左右军成两翼随行，中军上弓箭手，后军驻扎保护粮草。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80040, "groupId":5, "nPCLeftname":"_name", "nPCRightname":"士兵", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/15001", "leftEffect":1, "rightEffect":2, "talkingText":"得令……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80041, "groupId":6, "nPCLeftname":"", "nPCRightname":"地星-董卓", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/19001", "leftEffect":0, "rightEffect":3, "talkingText":"_name，仲颖恭候多时了。哈哈……哈哈……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80042, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":3, "rightEffect":1, "talkingText":"……[这是哪跟哪啊？]", "turn":1, "sound":"", "soundType":0 },
{ "ID":80043, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":1, "rightEffect":2, "talkingText":"_name，愚兄一直想与你结交，今日总于盼到你来了，哈哈……哈哈……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80044, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":2, "rightEffect":1, "talkingText":"……[葫芦里卖的什么药？先听听他说什么]", "turn":1, "sound":"", "soundType":0 },
{ "ID":80045, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":1, "rightEffect":2, "talkingText":"_name，你斩<color=#2652d5>张角</color>，战<color=#2652d5>吕布</color>现在名扬四海，n&如果你我能联手愚兄愿和你平分天下。n&不知贤弟意下如何？", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80046, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":2, "rightEffect":1, "talkingText":"呸……<color=#2652d5>董卓</color>，你废少帝立汉献帝并挟持号令，真乱臣贼子。", "turn":1, "sound":"", "soundType":0 },
{ "ID":80047, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":1, "rightEffect":2, "talkingText":"_name贤弟，汉室江山气数已尽，即使历史上没有我<color=#2652d5>董卓</color>也会有李卓、赵卓。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80048, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":1, "rightEffect":2, "talkingText":"你看四方军阀四起，又何必都怪罪到我头上。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80049, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":1, "rightEffect":2, "talkingText":"鸾凤独树一帜，不屑于凡鸟为伍。若你不听我好言相劝，只怕……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80050, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":2, "rightEffect":1, "talkingText":"只怕什么？", "turn":1, "sound":"", "soundType":0 },
{ "ID":80051, "groupId":6, "nPCLeftname":"_name", "nPCRightname":"地星-董卓", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/19001", "leftEffect":1, "rightEffect":2, "talkingText":"哼哼……只怕我不能留你在人间，做我日后绊脚石……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80052, "groupId":7, "nPCLeftname":"", "nPCRightname":"关羽", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/10007", "leftEffect":0, "rightEffect":3, "talkingText":"关某在此……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80053, "groupId":7, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":3, "rightEffect":1, "talkingText":"啊？？？二爷爷，你也来与我为难？[我终于见到我的偶像了，真是激动啊]", "turn":1, "sound":"", "soundType":0 },
{ "ID":80054, "groupId":7, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":1, "rightEffect":2, "talkingText":"关某并无此意……n&只是听说新出世了一位小英雄，武艺高强，名扬四海，n&关某想来领教一二。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80055, "groupId":7, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":2, "rightEffect":1, "talkingText":"二爷爷，我只是顺应历史啊，我哪敢跟你过招……[偶像真帅]", "turn":1, "sound":"", "soundType":0 },
{ "ID":80056, "groupId":7, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":1, "rightEffect":2, "talkingText":"关某既然来了，你总要给分薄面与我过两招。", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80057, "groupId":7, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":1, "rightEffect":2, "talkingText":"不要客套，让我关某见识一番……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80058, "groupId":8, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":1, "rightEffect":2, "talkingText":"后生可畏，关某告辞……", "turn":-1, "sound":"", "soundType":0 },
{ "ID":80059, "groupId":8, "nPCLeftname":"_name", "nPCRightname":"关羽", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10007", "leftEffect":2, "rightEffect":1, "talkingText":"……这个……哪个……[好激动啊……好激动啊……]", "turn":1, "sound":"", "soundType":0 },
{ "ID":80060, "groupId":9, "nPCLeftname":"关羽", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10007", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"前方有敌情，大家小心……", "turn":1, "sound":"Guide/10007_01", "soundType":2 },
{ "ID":80061, "groupId":9, "nPCLeftname":"刘备", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10009", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"二弟小心……", "turn":1, "sound":"Guide/10009_01", "soundType":2 },
{ "ID":80062, "groupId":9, "nPCLeftname":"张飞", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10008", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"怕什么怕，有俺老张在此，大哥二哥不必担心。", "turn":1, "sound":"Guide/10008_01", "soundType":2 },
{ "ID":80063, "groupId":9, "nPCLeftname":"", "nPCRightname":"吕布", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/10005", "leftEffect":0, "rightEffect":3, "talkingText":"哈哈哈，今天我要杀个痛快！", "turn":1, "sound":"Guide/10005_01", "soundType":2 },
{ "ID":80064, "groupId":9, "nPCLeftname":"", "nPCRightname":"吕布", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/10005", "leftEffect":0, "rightEffect":2, "talkingText":"你们几个一起上吧！", "turn":1, "sound":"Guide/10005_02", "soundType":2 },
{ "ID":80065, "groupId":10, "nPCLeftname":"关羽", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10007", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"大哥，三弟稍息片刻，待我取他项上人头。", "turn":1, "sound":"Guide/10007_02", "soundType":2 },
{ "ID":80066, "groupId":11, "nPCLeftname":"", "nPCRightname":"董卓", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/19001", "leftEffect":0, "rightEffect":3, "talkingText":"能打败我儿吕布，看来有两下子。", "turn":1, "sound":"Guide/10015_01", "soundType":2 },
{ "ID":80067, "groupId":11, "nPCLeftname":"张飞", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10008", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"哇呀呀……废话少说，速战速决。", "turn":1, "sound":"Guide/10008_02", "soundType":2 },
{ "ID":80068, "groupId":12, "nPCLeftname":"张飞", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10008", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"<color=#2652d5>张翼德</color>在此！", "turn":1, "sound":"Guide/10008_03", "soundType":2 },
{ "ID":80069, "groupId":13, "nPCLeftname":"", "nPCRightname":"董卓", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/19001", "leftEffect":0, "rightEffect":3, "talkingText":"看来我要送你们一程了。", "turn":1, "sound":"Guide/10015_02", "soundType":2 },
{ "ID":80070, "groupId":14, "nPCLeftname":"小乔", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10048", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"哇……好疼呀……", "turn":1, "sound":"Guide/10048_01", "soundType":2 },
{ "ID":80071, "groupId":14, "nPCLeftname":"王元姬", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10025", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"不要慌张，看我的治愈之泉。", "turn":1, "sound":"Guide/10025_01", "soundType":2 },
{ "ID":80072, "groupId":15, "nPCLeftname":"主公", "nPCRightname":"", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"大家别慌，我来助战！", "turn":1, "sound":"Guide/Master01_", "soundType":1 },
{ "ID":80073, "groupId":16, "nPCLeftname":"", "nPCRightname":"关羽", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/10007", "leftEffect":0, "rightEffect":3, "talkingText":"这一战打的漂亮，社稷统一大业一定会达成的。", "turn":1, "sound":"Guide/10007_03", "soundType":2 },
{ "ID":80074, "groupId":16, "nPCLeftname":"", "nPCRightname":"刘备", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"", "rightRes":"Plot/10009", "leftEffect":0, "rightEffect":3, "talkingText":"哈哈，痛快，不过我先回蜀地去了，来日再见。", "turn":1, "sound":"Guide/10009_02", "soundType":2 },
{ "ID":80075, "groupId":16, "nPCLeftname":"主公", "nPCRightname":"", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"等等……我还有点懵，好像有点乱套，这……这是三国时代？", "turn":1, "sound":"Guide/Master02_", "soundType":1 },
{ "ID":80076, "groupId":16, "nPCLeftname":"主公", "nPCRightname":"引导员", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10065", "leftEffect":1, "rightEffect":3, "talkingText":"呵呵呵……主公莫慌，今晚乾坤逆转，时光交替。你被历史选中，这是你的天命呀。", "turn":1, "sound":"Guide/10065_01", "soundType":2 },
{ "ID":80077, "groupId":16, "nPCLeftname":"主公", "nPCRightname":"引导员", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10065", "leftEffect":1, "rightEffect":2, "talkingText":"现在三国时期英雄齐聚一堂，九州风云变化莫测，历史怎么上演就全看你的了。", "turn":1, "sound":"Guide/10065_02", "soundType":2 },
{ "ID":80078, "groupId":16, "nPCLeftname":"主公", "nPCRightname":"引导员", "npcLeftType":1, "npcRightType":2, "plotType":1, "leftRes":"Plot/101", "rightRes":"Plot/10065", "leftEffect":1, "rightEffect":2, "talkingText":"既来之则安之，人无名不立，先选一个响亮的<color=#2652d5>旗号</color>吧！", "turn":1, "sound":"Guide/10065_03", "soundType":2 },
{ "ID":80079, "groupId":17, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"由于你驱赶了敌军，这个城里的百姓对你的十分敬爱，所以送来了<color=#2652d5>金币</color>和<color=#2652d5>铁矿</color>。", "turn":1, "sound":"Guide/10065_04", "soundType":2 },
{ "ID":80080, "groupId":18, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"有了金币和铁矿就能建设<color=#2652d5>主殿</color>了，这个城池就是你建功立业的根基了！", "turn":1, "sound":"Guide/10065_05", "soundType":2 },
{ "ID":80081, "groupId":19, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"主城建设好了，你的名声大振，一些英雄慕名前来投靠与你。", "turn":0, "sound":"Guide/10065_06", "soundType":2 },
{ "ID":80082, "groupId":19, "nPCLeftname":"徐庶", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10026", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"<color=#2652d5>徐元直</color>前来投靠，主公如不嫌弃，愿意成为麾下一员，听候调遣。", "turn":0, "sound":"Guide/10026_01", "soundType":2 },
{ "ID":80083, "groupId":19, "nPCLeftname":"黄盖", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10047", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"哇呀呀……俺来晚一步。主公，俺<color=#2652d5>老黄</color>愿意为你鞍前马后，斩将杀敌。", "turn":0, "sound":"Guide/10047_01", "soundType":2 },
{ "ID":80084, "groupId":19, "nPCLeftname":"甄姬", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10003", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"小女子也来投靠，愿为主公效劳。（看到主公小鹿乱跳……嘻嘻嘻……）", "turn":0, "sound":"Guide/10003_01", "soundType":2 },
{ "ID":80085, "groupId":20, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"我们从<color=#2652d5>正殿</color>开始吧。", "turn":0, "sound":"Guide/10065_07", "soundType":2 },
{ "ID":80086, "groupId":21, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"<color=#2652d5>钱庄</color>能生产铜币，为了保证军资充裕，要记住升级。", "turn":0, "sound":"Guide/10065_08", "soundType":2 },
{ "ID":80087, "groupId":22, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"想要发展科技，先要提升<color=#2652d5>兵部</color>的等级。", "turn":0, "sound":"Guide/10065_09", "soundType":2 },
{ "ID":80088, "groupId":23, "nPCLeftname":"引导员", "nPCRightname":"", "npcLeftType":2, "npcRightType":2, "plotType":1, "leftRes":"Plot/10065", "rightRes":"", "leftEffect":3, "rightEffect":0, "talkingText":"在你的带领下，我军士气高涨实力大增，将士们需要更高的挑战！", "turn":0, "sound":"Guide/10065_10", "soundType":2 }]